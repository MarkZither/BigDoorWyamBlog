@{
        string Title = Model.WithoutSettings.String(BlogKeys.Title);
        string BlogImage = Model.WithoutSettings.String(BlogKeys.Image);
        string Lead = (string.IsNullOrWhiteSpace(Model.WithoutSettings.String(BlogKeys.Lead)) ?
                @"Find a Property & Credit Agent who speaks your language.
                Save on Fees & Qualify for Rebates
                It's free and always will be." : Model.WithoutSettings.String(BlogKeys.Lead));
        string PageTitle =  (string.IsNullOrWhiteSpace(Title) ? Context.String(BlogKeys.Title) : Title);
}

<!DOCTYPE html>
<html lang="en">
        <head>
        <title>@PageTitle</title>
        <meta name="format-detection" content="telephone=no">
